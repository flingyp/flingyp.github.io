  "license": "MIT",
  "bugs": {
    "url": "https://github.com/github.com/flingyp/issues"
  },
  "homepage": "https://github.com/github.com/flingyp#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "commitizen": "^4.3.0",
    "esno": "^0.16.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "vitepress": "1.0.0-rc.4",
    "vue": "^3.3.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
